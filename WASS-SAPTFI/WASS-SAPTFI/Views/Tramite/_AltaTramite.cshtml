@model WASS_SAPTFI.ViewModels.Tramite.TramiteVM


@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <form class="form-inline">
        @{
            var listaTT = new SelectList(Model.Lista_Tipos_Tramite, "Id", "Descripcion");
        }
        
        <div class="form-group">
            @Html.DropDownListFor(m => m.Tipo_Tramite.Descripcion, listaTT, "Seleccionar Tipo", new { @class = "form-control" })
        </div>

            <p>
                <input type="submit" value="Create" />
            </p>
        
    </form>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}